<div id="main" class="row">
    <div id="sideMenuStatic" class="mainComponent">
        <div>
            <hamburger-toggle id="hamburger" ng-click="toggleNav()" state="hamState"></hamburger-toggle>
            <a id="goToReviewIcon" href="#!/view1" ng-click="closeNav()"></a>
            <a id="goToValidateIcon" href="#!/login" ng-click="closeNav()"></a>
        </div>
    </div>

    <div id="ng-view" class="mainComponent" ng-view></div>
