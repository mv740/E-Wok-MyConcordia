@{
    ViewData["Title"] = "Home Page";
}

<div id="mySidenav" class="sidenav">
    <!--<a href="javascript:void(0)" class="closebtn" ng-click="closeNav()">&times;</a>-->
    <a ng-click="toggleNav()">Close</a>
    <a href="#!/view1" ng-click="closeNav()">Review</a>
    <a href="#!/login" ng-click="closeNav()">Validate</a>

    @*@await Html.PartialAsync("_LoginPartial")*@
</div>

<div id="myModal" class="modal fade" role="dialog" ng-include="'partials/searchModal/searchModal.html'"></div>
